<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xx.demoproject.demofactory">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.RainbowBarActivity" />
        <activity android:name=".activities.CustomViewGroupActivity" />
        <activity android:name=".activities.SlotGameActivity" />
        <activity android:name=".activities.SurfaceViewDemoActivity" />
        <activity android:name=".activities.HandsupNotificationActivity" />

        <service android:name=".activities.FBService" />

        <activity android:name=".activities.BlurViewActivity" />
        <activity
            android:name=".activities.ButterKnifeDemoActivity"
            android:label="@string/title_activity_butter_knife_demo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".mvp.view.UserManagerActivity"></activity>
    </application>

</manifest>